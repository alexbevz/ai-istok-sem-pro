meta {
  name: find proxime items by id
  type: http
  seq: 14
}

get {
  url: {{base_url}}/api/v0/sps/collections/{{collection_id}}/items/{{item_id}}/find?count=10&limit_accuracy=0.1
  body: json
  auth: bearer
}

query {
  count: 10
  limit_accuracy: 0.1
}

auth:bearer {
  token: {{token}}
}

vars:pre-request {
  collection_id: 1
  item_id: 1
}
